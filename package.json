{
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.29.2",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "build": "pnpm --filter crawlee-one build",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "lint:fix": "pnpm -r lint:fix",
    "coverage": "pnpm --filter crawlee-one coverage",
    "bench": "pnpm --filter crawlee-one bench",
    "bench:gen": "pnpm --filter crawlee-one bench:gen",
    "validate": "tsx scripts/validate/index.ts",
    "docs:gen": "pnpm --filter crawlee-one docs:gen"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "puppeteer"
    ]
  },
  "devDependencies": {
    "@crawlee/browser-pool": "^3.16.0",
    "@crawlee/core": "^3.16.0",
    "@eslint/js": "^9.39.2",
    "@types/lodash": "^4.17.23",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^25.2.2",
    "@vitest/coverage-v8": "^4.0.18",
    "crawlee": "^3.16.0",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.0.0",
    "eslint-plugin-prettier": "^5.5.5",
    "playwright": "^1.58.2",
    "prettier": "^3.8.1",
    "puppeteer": "^24.37.2",
    "tsup": "^8.5.1",
    "tsx": "^4.21.0",
    "typedoc": "^0.28.16",
    "typedoc-plugin-markdown": "^4.10.0",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "vitest": "^4.0.18",
    "yaml": "^2.8.2"
  }
}