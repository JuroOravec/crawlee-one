{
  "name": "apify-actor-config",
  "version": "2.0.0",
  "type": "module",
  "private": false,
  "description": "Type-safe Apify Actor config. Full autocomplete, compile-time validation.",
  "author": "Juro Oravec <juraj.oravec.josefson@gmail.com>",
  "homepage": "https://github.com/JuroOravec/crawlee-one/tree/main/packages/apify-actor-config#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JuroOravec/crawlee-one.git",
    "directory": "packages/apify-actor-config"
  },
  "bugs": {
    "url": "https://github.com/JuroOravec/crawlee-one/issues"
  },
  "license": "MIT",
  "keywords": [
    "apify",
    "actor",
    "config",
    "actor-config",
    "input-schema",
    "output-schema"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "bin": {
    "apify-actor-config": "dist/cli.js"
  },
  "types": "dist/index.d.ts",
  "scripts": {
    "start": "tsx ./src/index.ts",
    "build": "tsup && tsc -p tsconfig.build.json",
    "lint": "eslint ./src",
    "lint:fix": "eslint ./src --fix",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "docs:gen": "typedoc src/index.ts --plugin typedoc-plugin-markdown --tsconfig tsconfig.json --gitRevision main --out ./docs/typedoc"
  },
  "dependencies": {
    "commander": "catalog:"
  }
}